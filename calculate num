//function for caltulate num 

function calculate(num1,num2,operator){
//if condition for calculate num 

    if(operator=='+'){
    console.log(num1+num2);
  }//else if condition for calculation
  
  else if(operator=='-'){
  console.log(num1/num2);
  }
  else if(operator=='/'){
  console.log(num1-num2);
  }
  else if(operator=='*'){
  console.log(num1*num2);
  }
  else if (operator==''){
    console.log("Null");
  }
  }
